function mongoSchemaRobo() {
  const mongoose = require("mongoose");
  var roboSchema = new mongoose.Schema({
    roboName: {
      type: String,
      require: true,
    },
    sequencias: [
      {
        seq: {
          type: String,
          require: true,
        },
        words: [],
        response:[],
        field: {
          type: String,
          require: true,
        },
        validator: {
          type: String,
          require: true,
        },
      },
    ],
  });
  //Collection
  return mongoose.model("robo", roboSchema);
}

module.exports = mongoSchemaRobo;
